{
	"name": "@mjljm/configs",
	"exports": {
		"./tsconfig-library.json": {
			"require": "./src/tsconfig-library.json"
		},
		"./tsconfig-node.json": {
			"require": "./src/tsconfig-node.json"
		},
		"./tsconfig-browser.json": {
			"require": "./src/tsconfig-browser.json"
		},
		"./.prettierrc": {
			"import": "./src/prettier-config.js",
			"types": {
				"import": "./src/prettier-config.d.ts"
			}
		}
	},
	"imports": {
		"#mjljm/configs/*": {
			"import": "./src/*.ts"
		}
	},
	"devDependencies": {
		"@effect/language-service": "0.0.21",
		"@html-eslint/eslint-plugin": "0.21.0",
		"@html-eslint/parser": "0.21.0",
		"@tsconfig/node20": "20.1.2",
		"@tsconfig/strictest": "2.0.2",
		"@types/eslint": "8.44.7",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/eslint__js": "8.42.3",
		"@types/node": "20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-markdown": "3.0.1",
		"eslint-plugin-yml": "1.10.0",
		"prettier": "3.1.0",
		"terser": "^5.24.0",
		"typescript": "5.3.2",
		"vite": "5.0.3",
		"vite-plugin-dts": "3.6.3"
	},
	"type": "module",
	"packageManager": "pnpm@8.10.2",
	"scripts": {
		"build": "vite build"
	}
}
